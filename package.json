{
  "name": "waves-intro",
  "version": "1.1.17-rc.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/waves-intro.git"
  },
  "devDependencies": {
    "grunt": "~1.1.0"
  },
  "phet": {
    "requirejsNamespace": "WAVES_INTRO",
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "phetLibs": [
      "wave-interference",
      "twixt",
      "griddle",
      "bamboo"
    ],
    "supportsSound": true,
    "supportsInteractiveDescriptions": true,
    "simulation": true,
    "screenNameKeys": [
      "WAVES_INTRO/water",
      "WAVES_INTRO/sound",
      "WAVES_INTRO/light"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_es6_eslintrc.js"
  }
}